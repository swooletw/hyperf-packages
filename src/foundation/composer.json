{
    "name": "swooletw/hyperf-foundation",
    "description": "The Support package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "foundation",
        "swoole",
        "larvel"
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert.cht@pm.me",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.16",
        "swooletw/hyperf-support": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/di": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/command": "~3.1.0",
        "hyperf/crontab": "~3.1.0",
        "hyperf/async-queue": "~3.1.0",
        "hyperf/collection": "~3.1.0",
        "hyperf/context": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "hyperf/stringable": "~3.1.0",
        "hyperf/database": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/signal": "~3.1.0",
        "hyperf/engine": "^2.1",
        "hyperf/db-connection": "~3.1.0",
        "symfony/error-handler": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Foundation\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Foundation\\ConfigProvider",
            "providers": [
                "SwooleTW\\Hyperf\\Foundation\\Providers\\FoundationServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
